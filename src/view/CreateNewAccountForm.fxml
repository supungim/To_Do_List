<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="498.0" prefWidth="357.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CreateNewAccountFormController">
   <children>
      <Label layoutX="92.0" layoutY="27.0" text="Create New Account" underline="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="104.0" text="User Name">
         <font>
            <Font name="Leelawadee UI" size="13.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="177.0" text="E-Mail">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="250.0" text="New Password">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <Label layoutX="24.0" layoutY="326.0" text="Confirm Password" />
      <TextField fx:id="txtUserName" layoutX="24.0" layoutY="130.0" prefHeight="33.0" prefWidth="298.0" promptText="User Name" />
      <TextField fx:id="txtEmail" layoutX="27.0" layoutY="203.0" prefHeight="33.0" prefWidth="298.0" promptText="E Mail" />
      <TextField fx:id="txtNewPassword" accessibleRole="PASSWORD_FIELD" layoutX="27.0" layoutY="275.0" prefHeight="33.0" prefWidth="298.0" promptText="New Password" />
      <TextField fx:id="txtConformPassword" accessibleRole="PASSWORD_FIELD" layoutX="28.0" layoutY="349.0" onAction="#txtConfirmPasswordOnAction" prefHeight="33.0" prefWidth="290.0" promptText="Confirm Password" />
      <Button fx:id="btnRegister" layoutX="60.0" layoutY="412.0" mnemonicParsing="false" onAction="#btnRegisterOnAction" prefHeight="33.0" prefWidth="231.0" style="-fx-background-color: Green;" text="Register" textFill="WHITE" />
      <Label fx:id="lblpasswordNotMatch1" layoutX="199.0" layoutY="252.0" text="Password does Not Match" textFill="RED">
         <font>
            <Font name="System Bold" size="10.0" />
         </font></Label>
      <Label fx:id="lblpasswordNotMatch2" layoutX="195.0" layoutY="327.0" text="Password does Not Match" textFill="RED">
         <font>
            <Font name="System Bold" size="10.0" />
         </font></Label>
      <Button layoutX="195.0" layoutY="68.0" mnemonicParsing="false" onAction="#btnAddNewUserOnAction" prefHeight="33.0" prefWidth="123.0" style="-fx-background-color: Blue;" text="Add New User" textFill="WHITE" />
      <Label fx:id="lblId" layoutX="28.0" layoutY="68.0" text="User ID" />
   </children>
</AnchorPane>
