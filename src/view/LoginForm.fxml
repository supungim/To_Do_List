<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="460.0" prefWidth="346.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginFormController">
   <children>
      <Label layoutX="85.0" layoutY="33.0" prefHeight="27.0" prefWidth="191.0" text="Login To To-Do List" underline="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="21.0" layoutY="98.0" text="User Name">
         <font>
            <Font name="Leelawadee UI" size="13.0" />
         </font></Label>
      <Label layoutX="21.0" layoutY="198.0" text="Password" />
      <TextField fx:id="txtUserName" layoutX="18.0" layoutY="131.0" prefHeight="33.0" prefWidth="311.0" promptText="User Name" />
      <TextField fx:id="txtPassword" accessibleRole="PASSWORD_FIELD" layoutX="21.0" layoutY="227.0" onAction="#txtPasswordOnAction" prefHeight="33.0" prefWidth="305.0" promptText="Enter Password" />
      <Button layoutX="53.0" layoutY="294.0" mnemonicParsing="false" onAction="#btnLoginOnAction" prefHeight="41.0" prefWidth="242.0" style="-fx-background-color: blue;" text="Login" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="114.0" layoutY="358.0" onMouseClicked="#lblCreateNewAccountOnMouseClicked" text="Create New Account" textFill="#28068d" underline="true">
         <font>
            <Font name="Leelawadee UI Bold" size="13.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
</AnchorPane>
